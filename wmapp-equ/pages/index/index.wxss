/**index.wxss**/

/* 头部 */

.index-header {
}

.index-title {
  position: fixed;
  left: 0;
  top: 100rpx;
  width: 100%;
  z-index: 9;
  background-color: #fff;
}

.index-header .tab-item {
  color: #bad8d8;
}

.tab-h {
  height: 80rpx;
  width: 60%;
  padding: 0 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: #fff;
  font-size: 16px;
  white-space: nowrap;
}

.tab-h.tab-head {
  width: 80%;
}

.tab-content {
  /* margin-top: 80rpx; */
  padding: 0 30rpx;
}

.tab-item {
  display: inline-block;
  transition: all 0.3s;
  color: #bad8d8;
  font-size: 30rpx;
  font-weight: 600;
  /* width: 25%; */
  margin-right: 13%;
  text-align: center;
  box-sizing: border-box;
}

.tab-item:first-child {
  text-align: left;
}

.tab-item:last-child {
  text-align: right;
  margin-right: 0;
}

.tab-item.active {
  font-size: 32rpx;
  color: #070b28;
  position: relative;
}

.tab-head .tab-item.active {
  font-size: 36rpx;
}

.item-ans {
  width: 100%;
  display: flex;
  flex-grow: row no-wrap;
  justify-content: space-between;
  padding: 30rpx;
  box-sizing: border-box;
  height: 180rpx;
  align-items: center;
  border-bottom: 1px solid #f2f2f2;
}

.user-info {
  display: flex;
  margin: 0 0 20rpx;
  position: relative;
}

.user-info text {
  line-height: 76rpx;
  font-size: 26rpx;
  letter-spacing: 2rpx;
  font-weight: 600;
}

.avatar {
  width: 80rpx;
  height: 80rpx;
  position: relative;
  padding-right: 30rpx;
  /* display: inline-block */
}

.avatar .img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background-color: #fff;
}

.avatar .doyen {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: -2px;
  right: 20rpx;
}

.image-list {
  width: 100%;
  height: 390rpx;
  border-radius: 26rpx;
  overflow: hidden;
  margin: 0 0 20rpx;
  display: flex;
}

.image-list-video {
  background-color: #000;
}

.image-list.image-lists {
  height: 29vw;
}

.image-list.image-list2 {
  height: 49vw;
}

.image-list2 .lists {
  width: 50vw;
  height: 49vw;
  border-radius: 20rpx;
}

.image-lists .lists {
  width: 32vw;
  height: 29vw;
  margin-right: 1.4vw;
  border-radius: 20rpx;
}

.image-lists .lists:last-child {
  margin: 0;
}

.image-list video {
  width: 100%;
  height: 100%;
}

.image-list image {
  position: relative;
  /* background-color: #000; */
}

.container-list {
  margin-bottom: 42rpx;
}

.subject {
  color: #070c29;
  margin: 10rpx 0 0;
  font-weight: 600;
  font-size: 32rpx;
  text-indent: 0;
  padding: 0;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
  text-align: left;
}

.container-list .message {
  margin: 6rpx 0 0;
  line-height: 146%;
  font-size: 32rpx;
}

.footer {
  color: #b8d8d7;
  font-size: 22rpx;
  display: flex;
  position: relative;
  margin: 14rpx 0 0;
  min-height: 42rpx;
}

.view-reply {
  display: flex;
  font-weight: 300;
}

.footer .view-reply {
  position: absolute;
  right: 0;
  top: 0;
}

.view-reply image {
  width: 34rpx;
  height: 34rpx;
  align-items: center;
  margin-right: 16rpx;
}

.image .view-reply view {
  color: #fff;
}

.view-reply view {
  width: 110rpx;
  line-height: 40rpx;
  align-items: center;
  display: flex;
  font-size: 22rpx;
  color: #b8d8d7;
}

.footers {
  position: relative;
}

.footers .view-reply .time {
  position: absolute;
  right: 0;
  top: 0;
  text-align: right;
  display: inline;
  width: auto;
}

.view-reply .replies {
  width: 100rpx;
}

.view-reply .shares {
  width: 34rpx;
}

.view-reply .shares image {
  margin: 0;
}

.expertInfo {
  font-size: 12px;
  flex-grow: 2;
  color: #b0b0b0;
  line-height: 1.5em;
}

.expertInfo .name {
  font-size: 32rpx;
  color: #000;
  margin-bottom: 6px;
}

.askBtn {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 14px;
  border-radius: 60rpx;
  border: 1px solid #4675f9;
  color: #4675f9;
}

.scoll-h {
  /* height: 100%; */
  height: calc(100% + 80rpx);
  transform: translateY(-80rpx);
  transition: all 0.4s;
}

/* 头部end */

/* 选择卡 */

.area {
  top: -108vh;
  left: -100vw;
  height: 100vh;
  width: 190vw;
  background: rgba(0, 0, 0, 0);
}

.mo_top {
  position: absolute;
  top: 0;
  left: 0;
  height: 100vh;
  width: 100vw;
  background: rgba(0, 0, 0, 0);
}

.image {
  position: relative;
  width: 590rpx;
  height: 622rpx;
  border-radius: 20rpx;
  text-align: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin: 160rpx auto 0;
}

movable-view:nth-child(3n+1) .image {
  background-color: #5ee2d4;
  /* margin-top: 0; */
}

movable-view:nth-child(3n) .image {
  background-color: #a4a8ff;
  /* margin-top: 30rpx; */
}

movable-view:nth-child(3n+2) .image {
  background-color: #ffd7f4;
  /* margin-top: 60rpx; */
}

.image .imgs {
  height: 55%;
  width: 100%;
  border-radius: 20rpx;
}

.image .imgs .video-icon {
  width: 13vw;
  height: 13vw;
  left: 41%;
  top: 34%;
}

.big .avatar {
  position: absolute;
  top: -30rpx;
  left: 20rpx;
  width: 100rpx;
  height: 100rpx;
}

.no-cover .big {
  margin: 78rpx 0;
}

.no-cover .big .avatar {
  top: 0;
  left: 0;
}

.no-cover .big .user-info text {
  margin-top: 8rpx;
}

.big .avatar .img {
  border: 6rpx solid #fff;
}

.big .user-info {
  padding-left: 144rpx;
  margin: 0 0 38rpx;
  position: relative;
}

.big .user-info text {
  line-height: 70rpx;
  font-size: 30rpx;
}

.big .subject {
  padding: 0 10rpx;
  margin: 6rpx 0 22rpx;
  height: 80rpx;
  font-size: 30rpx;
  line-height: 44rpx;
}

.image .footer {
  color: #fff;
  font-size: 24rpx;
  padding: 0 10rpx;
}

.image .footer .replies {
}

.image .footer .replies image {
  margin-left: 18%;
}

.content_s {
  z-index: 999;
}

movable-view {
  width: 0;
  height: 0;
  z-index: 1;
}

.index-content {
  margin-top: 80rpx;
}

.section {
  height: 680rpx;
  width: 600rpx;
  margin: 0 auto 0;
}

