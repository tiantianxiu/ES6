/* 选择卡END */

.shua-text {
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20%;
  position: relative;
}

.zan-loading {
  position: absolute;
  bottom: 22%;
  left: 14%;
}

.thread-class {
  width: 100%;
  padding-left: 30rpx;
  /* overflow: hidden; */
  /* overflow-x: hidden; */
  white-space: nowrap;
  box-sizing: border-box;
}

.thread-class .list {
  width: 30vw;
  display: inline-block;
  height: 30vw;
  margin-right: 5vw;
  border-radius: 20rpx;
  overflow: hidden;
}

.index-list {
  /* transition: all .3s; */
  background: #fff;
  position: relative;
  z-index: 10;
  margin-top: -118rpx;
  /* margin-top: 80rpx; */
}

.index-list-active {
  position: fixed;
  left: 0;
  z-index: 10;
  width: 100%;
  margin-top: 0;
}

.x {
  position: absolute;
  right: 0;
  top: 0;
  height: 120rpx;
  width: 200rpx;
  text-align: right;
}

.x image {
  margin: 20rpx 40rpx 0 0;
  height: 30rpx;
  width: 30rpx;
}



.square-line {
  width: 80%;
  margin: 30rpx auto;
  border-bottom: 1rpx solid #bad8d8;
}

.square-list {
  padding: 40rpx 0;
}

.square-list .image-lists {
  display: block;
  height: auto;
}

.square-list .image-list {
  width: 66%;
}

.square-list .image-list.image-list1 {
  width: 42vw;
  height: 42vw;
}

.square-list .image-list.image-list-video {
  width: 36vw;
  height: 56vw;
}

.image-list-video .video-icon {
  width: 12vw;
  height: 12vw;
  left: 43.5%;
  position: absolute;
  top: 38%;
}

.square-list .image-lists.image-list4 {
  width: 66%;
  display: block;
  height: auto;
}

.square-list .image-lists .lists {
  width: 22vw;
  height: 22vw;
}

.square-content {
  padding-left: 110rpx;
}

.ad-image {
  border-radius: 30rpx;
  height: 280rpx;
  position: relative;
  overflow: hidden;
  margin: 48rpx 0;
}

.ad-image text {
  position: absolute;
  right: 0;
  top: 0;
  border-top-right-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  padding: 0 10px;
  background-color: #11b9ba;
  color: #fff;
  font-size: 24rpx;
}

.class-name {
  color: #4440f0;
}
